{
  "version": 2,
  "builds": [
    {
      "src": "static/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "static/build" }
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/build/index.html"
    }
  ]
}
