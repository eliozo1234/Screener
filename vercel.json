{
  "version": 2,
  "builds": [
    {
      "src": "app/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "static/index.html"
    }
  ]
}
